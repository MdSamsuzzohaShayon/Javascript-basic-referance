{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier",
        "plugin:node/remcommended"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "warn",
        "object-shorthand": "off"
    },
    "parserOptions": {
        "ecmaVersion": 12
    }
}
